{
  "name": "@mapbox/probematch",
  "version": "3.0.0",
  "description": "A small module for matching probes to road networks",
  "type": "module",
  "main": "probematch.js",
  "scripts": {
    "lint": "eslint probematch.js test/*.js",
    "pretest": "npm run lint",
    "test": "node test/test.js",
    "bench": "node bench/searching.js 10"
  },
  "eslintConfig": {
    "extends": "mourner",
    "rules": {
      "strict": [
        0
      ],
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "new-cap": [
        0
      ]
    }
  },
  "author": "Tim Channell",
  "license": "ISC",
  "dependencies": {
    "@turf/bbox": "^6.0.1",
    "cheap-ruler": "^3.0.2",
    "flatbush": "^4.4.0",
    "turf-linestring": "^1.0.0",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-mourner": "^3.0.0",
    "geojson-random": "^0.5.0",
    "turf-point": "^2.0.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapbox/probematch.git"
  },
  "bugs": {
    "url": "https://github.com/mapbox/probematch/issues"
  },
  "homepage": "https://github.com/mapbox/probematch#readme",
  "keywords": [
    "gps",
    "match",
    "probe",
    "geojson"
  ]
}
